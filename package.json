{
  "name": "msg-interface",
  "description": "Msgpack Container Interface",
  "version": "0.99.714",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/msg-interface/issues"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "jshint": "^2.9.6",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "zuul": "^3.12.0"
  },
  "engines": {
    "node": ">= 4.5.0"
  },
  "homepage": "https://github.com/kawanet/msg-interface#readme",
  "jshintConfig": {
    "esversion": 6,
    "mocha": true,
    "node": true,
    "proto": true,
    "undef": true,
    "unused": true,
    "validthis": true
  },
  "keywords": [
    "messagepack",
    "msgpack"
  ],
  "license": "MIT",
  "main": "./msg-interface.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/msg-interface.git"
  },
  "scripts": {
    "coveralls": "./node_modules/.bin/nyc npm run mocha && ./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint .",
    "mocha": "./node_modules/.bin/mocha test/*.js",
    "test": "npm run jshint && npm run mocha",
    "zuul-local": "./node_modules/.bin/zuul --local 4000 --ui mocha-bdd test/*.js"
  },
  "typings": "./msg-interface.d.ts"
}
