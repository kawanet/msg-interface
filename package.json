{
  "name": "msg-interface",
  "description": "Msgpack Container Interface",
  "version": "0.99.714",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/msg-interface/issues"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.2",
    "coveralls": "^3.0.2",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "typescript": "^2.9.2",
    "zuul": "^3.12.0"
  },
  "homepage": "https://github.com/kawanet/msg-interface#readme",
  "jshintConfig": {
    "mocha": true,
    "node": true,
    "proto": true,
    "undef": true,
    "unused": true,
    "validthis": true
  },
  "keywords": [
    "messagepack",
    "msgpack"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/msg-interface.git"
  },
  "scripts": {
    "coveralls": "./node_modules/.bin/nyc npm run mocha && ./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls",
    "fixpack": "fixpack",
    "jshint": "./node_modules/.bin/jshint .",
    "mocha": "./node_modules/.bin/mocha test/*.js",
    "test": "npm run tsc && npm run jshint && npm run mocha",
    "tsc": "./node_modules/.bin/tsc",
    "tsc-watch": "./node_modules/.bin/tsc --watch",
    "zuul-local": "./node_modules/.bin/zuul --local 4000 --ui mocha-bdd test/*.js"
  },
  "typings": "typings/msg-interface.d.ts"
}
